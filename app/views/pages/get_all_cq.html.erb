<h2></h2>
<div class="ui container">
    <div class="ui divided relaxed list">
             <h1>Crate Queries</h1>
             <%if @q_noti.length == 0%>
                <h1></h1>
                Nothing to show
             <%else%>
                  <%@q_noti.each do |notif| %>
                  <div class="item">
                    <div class="content">
                      <div class="description"><%=notif.owner.alias%> <%=render_activity notif%></div>
                    </div>
                  </div>
                  <%end%>
              <%end%>
           </div>
</div>